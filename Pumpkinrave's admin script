wait("0.2")

--// GUI Setup
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 200, 0, 200)
Frame.Position = UDim2.new(0.5, -100, 0.5, -100)
Frame.BackgroundColor3 = Color3.fromRGB(255, 165, 0)
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 30)
Title.BackgroundTransparency = 1
Title.Text = "Pumpkinraves Admin"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 18
Title.Parent = Frame

-- Buttons
local flyButton = Instance.new("TextButton")
flyButton.Size = UDim2.new(1, 0, 0, 30)
flyButton.Position = UDim2.new(0, 0, 0, 30)
flyButton.Text = "Fly"
flyButton.Parent = Frame

local noclipButton = Instance.new("TextButton")
noclipButton.Size = UDim2.new(1, 0, 0, 30)
noclipButton.Position = UDim2.new(0, 0, 0, 60)
noclipButton.Text = "Noclip"
noclipButton.Parent = Frame

local speedButton = Instance.new("TextButton")
speedButton.Size = UDim2.new(1, 0, 0, 30)
speedButton.Position = UDim2.new(0, 0, 0, 30)
speedButton.Text = "Speed +2"
speedButton.Parent = Frame

local jumpButton = Instance.new("TextButton")
jumpButton.Size = UDim2.new(1, 0, 0, 30)
jumpButton.Position = UDim2.new(0, 0, 0, 60)
jumpButton.Text = "Jump +2"
jumpButton.Parent = Frame

local espButton = Instance.new("TextButton")
espButton.Size = UDim2.new(1, 0, 0, 30)
espButton.Position = UDim2.new(0, 0, 0, 30)
espButton.Text = "ESP"
espButton.Parent = Frame

local hpButton = Instance.new("TextButton")
hpButton.Size = UDim2.new(1, 0, 0, 30)
hpButton.Position = UDim2.new(0, 0, 0, 60)
hpButton.Text = "10K HP"
hpButton.Parent = Frame

local nextButton = Instance.new("TextButton")
nextButton.Size = UDim2.new(1, 0, 0, 30)
nextButton.Position = UDim2.new(0, 0, 0, 150)
nextButton.Text = "Next"
nextButton.Parent = Frame

-- Page system
local currentPage = 1
local function updatePage()
    flyButton.Visible = false
    noclipButton.Visible = false
    speedButton.Visible = false
    jumpButton.Visible = false
    espButton.Visible = false
    hpButton.Visible = false

    if currentPage == 1 then
        flyButton.Visible = true
        noclipButton.Visible = true
    elseif currentPage == 2 then
        speedButton.Visible = true
        jumpButton.Visible = true
    elseif currentPage == 3 then
        espButton.Visible = true
        hpButton.Visible = true
    end
end

nextButton.MouseButton1Click:Connect(function()
    currentPage += 1
    if currentPage > 3 then
        currentPage = 1
    end
    updatePage()
end)

updatePage()

-- Fly Function
local flying = false
local uis = game:GetService("UserInputService")
local rs = game:GetService("RunService")
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

flyButton.MouseButton1Click:Connect(function()
    flying = not flying
    local bv = Instance.new("BodyVelocity")
    bv.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    bv.Velocity = Vector3.zero
    bv.Parent = hrp

    rs.RenderStepped:Connect(function()
        if flying then
            local move = Vector3.zero
            if uis:IsKeyDown(Enum.KeyCode.W) then move = move + (hrp.CFrame.LookVector) end
            if uis:IsKeyDown(Enum.KeyCode.S) then move = move - (hrp.CFrame.LookVector) end
            if uis:IsKeyDown(Enum.KeyCode.A) then move = move - (hrp.CFrame.RightVector) end
            if uis:IsKeyDown(Enum.KeyCode.D) then move = move + (hrp.CFrame.RightVector) end
            bv.Velocity = move * 50
        else
            bv:Destroy()
        end
    end)
end)

-- Noclip
local noclip = false
noclipButton.MouseButton1Click:Connect(function()
    noclip = not noclip
    rs.Stepped:Connect(function()
        if noclip then
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
end)

-- Speed +2
speedButton.MouseButton1Click:Connect(function()
    char.Humanoid.WalkSpeed = char.Humanoid.WalkSpeed + 2
end)

-- Jump +2
jumpButton.MouseButton1Click:Connect(function()
    char.Humanoid.JumpPower = char.Humanoid.JumpPower + 2
end)

-- ESP
espButton.MouseButton1Click:Connect(function()
    for _, plr in pairs(game.Players:GetPlayers()) do
        if plr ~= player and plr.Character then
            local highlight = Instance.new("Highlight")
            highlight.FillColor = Color3.fromRGB(255, 165, 0)
            highlight.OutlineColor = Color3.new(1, 1, 1)
            highlight.Parent = plr.Character
        end
    end
end)

-- 10K HP
hpButton.MouseButton1Click:Connect(function()
    char.Humanoid.MaxHealth = 10000
    char.Humanoid.Health = 10000
end)
